<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.daryl.mapper.SecondHouseMapper">

    <select id="queryUserHouseByUserId" resultType="com.daryl.domain.entity.SecondHouseEntity">
        select * from t_second_house where del_tag = 0 and user_id = #{userId} order by create_time desc
    </select>
</mapper>
